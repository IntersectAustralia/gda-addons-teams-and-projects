<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
    >
     <bean id="fedoraXmlTemplateBuilder" 
     class="au.org.intersect.gda.repository.fedora.FedoraXmlTemplateBuilderImpl">
        <constructor-arg name="templateName" value="template.xml"/>
     </bean>    
   
     <bean id="fedoraOaiTemplateBuilder" 
     class="au.org.intersect.gda.repository.fedora.FedoraXmlTemplateBuilderImpl">
        <constructor-arg name="templateName" value="oaiObjectTemplate.xml"/>
     </bean>    
   
     <bean id="fedoraRelsExtTemplateBuilder" 
     class="au.org.intersect.gda.repository.fedora.FedoraXmlTemplateBuilderImpl">
        <constructor-arg name="templateName" value="rels-ext.xml"/>
     </bean>    
   
</beans>